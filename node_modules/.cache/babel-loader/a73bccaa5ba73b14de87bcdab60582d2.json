,EAAA,OAAA,EAAA;AACA,IAAA,WADA,yBACA;AACA,WAAA,KAAA,CAAA,eAAA,EAAA,KAAA;AACA,KAHA;AAKA,IAAA,IALA,kBAKA;AAAA;;AACA,UAAA,KAAA,IAAA,EAAA;AACA,aAAA,MAAA,CACA,QADA,CACA,gCADA,EACA,KAAA,QADA,EAEA,IAFA,CAEA,YAAA;AACA,UAAA,KAAA,CAAA,MAAA,CAAA,OAAA,CAAA,oBAAA,EAAA;AACA,YAAA,QAAA,EAAA;AADA,WAAA;;AAGA,UAAA,KAAA,CAAA,KAAA,CAAA,IAAA,CAAA,KAAA;;AACA,UAAA,KAAA,CAAA,MAAA,CACA,QADA,CACA,iCADA,EAEA,IAFA,CAEA,YAAA;AACA,YAAA,KAAA,CAAA,WAAA;AACA,WAJA,EAKA,KALA,CAKA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,CAAA,MAAA,CAAA,KAAA,8CAAA,KAAA,GAAA;AACA,cAAA,QAAA,EAAA;AADA,aAAA;AAGA,WATA;AAUA,SAjBA;AAkBA,OAnBA,MAmBA;AACA,aAAA,MAAA,CACA,QADA,CACA,8BADA,EACA,KAAA,QADA,EAEA,IAFA,CAEA,YAAA;AACA,UAAA,KAAA,CAAA,MAAA,CAAA,OAAA,CAAA,oBAAA,EAAA;AACA,YAAA,QAAA,EAAA;AADA,WAAA;;AAGA,UAAA,KAAA,CAAA,KAAA,CAAA,IAAA,CAAA,KAAA;;AACA,UAAA,KAAA,CAAA,MAAA,CACA,QADA,CACA,iCADA,EAEA,IAFA,CAEA,YAAA;AACA,YAAA,KAAA,CAAA,WAAA;AACA,WAJA,EAKA,KALA,CAKA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,CAAA,MAAA,CAAA,KAAA,8CAAA,KAAA,GAAA;AACA,cAAA,QAAA,EAAA;AADA,aAAA;AAGA,WATA;AAUA,SAjBA;AAkBA;AACA;AA7CA,GA1BA;AAyEA,EAAA,OAzEA,qBAyEA;AAAA;;AACA,QAAA,KAAA,IAAA,EAAA;AACA,WAAA,SAAA,GAAA,KAAA;AACA,WAAA,MAAA,CACA,QADA,CACA,kCADA,EACA,KAAA,UADA,EAEA,IAFA,CAEA,UAAA,CAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,CAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,KAAA;AACA,OALA,EAMA,KANA,CAMA,YAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,KAAA;AACA,OARA;AASA;AACA,GAtFA;AAuFA,EAAA,KAAA,EAAA;AACA,IAAA,MADA,oBACA;AACA,WAAA,WAAA,GAAA,KAAA,MAAA;AACA;AAHA;AAvFA,CAAA","sourcesContent":["<template>\n  <div>\n    <v-btn icon @click=\"$emit('update:dialog', true)\"\n      v-if=\"!edit\"><v-icon>mdi-plus</v-icon></v-btn\n    >\n\n    <v-dialog v-model=\"dialogModel\" width=\"500\" persistent>\n      <v-card>\n        <v-card-title class=\"headline grey lighten-2\">\n          Portfolyo Kategori Ekle\n        </v-card-title>\n        <v-progress-circular\n          v-if=\"isLoading\"\n          color=\"primary\"\n          indeterminate\n        ></v-progress-circular>\n        <v-card-text v-else>\n          <v-form ref=\"form\">\n            <v-text-field label=\"İsim\" v-model=\"formData.name\"></v-text-field>\n            <v-autocomplete\n              item-text=\"name\"\n              item-value=\"id\"\n              :items=\"categories\"\n              label=\"Üst Kategorisi\"\n              v-model=\"formData.parent\"\n            ></v-autocomplete>\n            <v-textarea\n              label=\"Açıklama\"\n              v-model=\"formData.description\"\n            ></v-textarea>\n          </v-form>\n        </v-card-text>\n\n        <v-divider></v-divider>\n\n        <v-card-actions>\n          <v-spacer></v-spacer>\n          <v-btn color=\"primary\" text @click=\"save\"> Kaydet </v-btn>\n          <v-btn color=\"error\" text @click=\"closeDialog\"> İptal </v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n  </div>\n</template>\n\n<script>\nexport default {\n  props: {\n    dialog: {\n      default: false,\n    },\n    categories: {\n      default: [],\n    },\n    edit: {\n      default: false,\n    },\n    categoryId: {\n      type: Number,\n    },\n  },\n  data() {\n    return {\n      dialogModel: this.dialog,\n      isLoading: false,\n      formData: {\n        name: null,\n        description: null,\n        parent: null,\n      },\n    };\n  },\n  methods: {\n    closeDialog() {\n      this.$emit(\"update:dialog\", false);\n    },\n\n    save() {\n      if (this.edit) {\n        this.$store\n          .dispatch(\"PortfolioCategoryModule/update\", this.formData)\n          .then(() => {\n            this.$toast.success(\"Başarıyla Eklendi!\", {\n              position: \"top-right\",\n            });\n            this.$refs.form.reset();\n            this.$store\n              .dispatch(\"PortfolioCategoryModule/findAll\")\n              .then(() => {\n                this.closeDialog();\n              })\n              .catch((error) => {\n                this.$toast.error(`Kaydedilirken bir hata oluştu ${error}`, {\n                  position: \"top-right\",\n                });\n              });\n          });\n      } else {\n        this.$store\n          .dispatch(\"PortfolioCategoryModule/save\", this.formData)\n          .then(() => {\n            this.$toast.success(\"Başarıyla Eklendi!\", {\n              position: \"top-right\",\n            });\n            this.$refs.form.reset();\n            this.$store\n              .dispatch(\"PortfolioCategoryModule/findAll\")\n              .then(() => {\n                this.closeDialog();\n              })\n              .catch((error) => {\n                this.$toast.error(`Kaydedilirken bir hata oluştu ${error}`, {\n                  position: \"top-right\",\n                });\n              });\n          });\n      }\n    },\n  },\n  mounted() {\n    if (this.edit) {\n      this.isLoading = false;\n      this.$store\n        .dispatch(\"PortfolioCategoryModule/findById\", this.categoryId)\n        .then((e) => {\n          this.formData = e;\n          this.isLoading = false;\n        })\n        .catch(() => {\n          this.isLoading = false;\n        });\n    }\n  },\n  watch: {\n    dialog() {\n      this.dialogModel = this.dialog;\n    },\n  },\n};\n</script>\n\n<style>\n</style>"],"sourceRoot":"src/components/AddPortfolioCategoryDialog"}]}