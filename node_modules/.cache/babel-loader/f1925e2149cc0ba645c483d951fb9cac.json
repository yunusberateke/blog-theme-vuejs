AA;AADA,WAAA;;AAGA,UAAA,KAAA,CAAA,KAAA,CAAA,IAAA,CAAA,KAAA;;AACA,UAAA,KAAA,CAAA,MAAA,CACA,QADA,CACA,mBADA,EAEA,IAFA,CAEA,YAAA;AACA,YAAA,KAAA,CAAA,WAAA;AACA,WAJA,EAKA,KALA,CAKA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,CAAA,MAAA,CAAA,KAAA,8CAAA,KAAA,GAAA;AACA,cAAA,QAAA,EAAA;AADA,aAAA;AAGA,WATA;AAUA,SAfA;AAgBA,OAjBA,MAiBA;AACA,aAAA,MAAA,CAAA,QAAA,CAAA,gBAAA,EAAA,KAAA,QAAA,EAAA,IAAA,CAAA,YAAA;AACA,UAAA,KAAA,CAAA,MAAA,CAAA,OAAA,CAAA,oBAAA,EAAA;AACA,YAAA,QAAA,EAAA;AADA,WAAA;;AAGA,UAAA,KAAA,CAAA,KAAA,CAAA,IAAA,CAAA,KAAA;;AACA,UAAA,KAAA,CAAA,MAAA,CACA,QADA,CACA,mBADA,EAEA,IAFA,CAEA,YAAA;AACA,YAAA,KAAA,CAAA,WAAA;AACA,WAJA,EAKA,KALA,CAKA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,CAAA,MAAA,CAAA,KAAA,8CAAA,KAAA,GAAA;AACA,cAAA,QAAA,EAAA;AADA,aAAA;AAGA,WATA;AAUA,SAfA;AAgBA;AACA;AAxCA,GAfA;AAyDA,EAAA,IAzDA,kBAyDA;AACA,WAAA;AACA,MAAA,WAAA,EAAA,KAAA,MADA;AAGA,MAAA,QAAA,EAAA;AACA,QAAA,IAAA,EAAA;AADA;AAHA,KAAA;AAOA,GAjEA;AAkEA,EAAA,KAAA,EAAA;AACA,IAAA,MADA,oBACA;AACA,WAAA,WAAA,GAAA,KAAA,MAAA;AACA;AAHA,GAlEA;AAuEA,EAAA,OAvEA,qBAuEA;AAAA;;AACA,QAAA,KAAA,IAAA,EAAA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,oBAAA,EAAA;AAAA,QAAA,EAAA,EAAA,KAAA;AAAA,OAAA,EAAA,IAAA,CAAA,UAAA,CAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,CAAA;AACA,OAFA;AAGA;AACA;AA7EA,CAAA","sourcesContent":["<template>\n  <div>\n    <v-btn icon @click=\"$emit('update:dialog', true)\"\n      ><v-icon>mdi-plus</v-icon></v-btn\n    >\n    <v-dialog v-model=\"dialog\" width=\"500\" persistent>\n      <v-card>\n        <v-card-title class=\"headline grey lighten-2\">Tag Ekle</v-card-title>\n        <v-card-text>\n          <v-form ref=\"form\">\n            <v-text-field label=\"İsim\" v-model=\"formData.name\"></v-text-field>\n          </v-form>\n        </v-card-text>\n        <v-card-actions>\n          <v-spacer></v-spacer>\n          <v-btn color=\"primary\" text @click=\"save\"> Kaydet </v-btn>\n          <v-btn color=\"error\" text @click=\"closeDialog\"> İptal </v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n  </div>\n</template>\n\n<script>\nexport default {\n  props: {\n    dialog: {\n      type: Boolean,\n      default: true,\n    },\n    edit: {\n      type: Boolean,\n      default: false,\n    },\n    tagId: {\n      type: Number,\n      default: 1,\n    }\n  },\n  methods: {\n    closeDialog() {\n      this.$emit(\"update:dialog\", false);\n    },\n    save() {\n      if (this.edit) {\n        this.$store.dispatch(\"TagModule/update\", this.formData).then(() => {\n          this.$toast.success(\"Başarıyla Eklendi!\", {\n            position: \"top-right\",\n          });\n          this.$refs.form.reset();\n          this.$store\n            .dispatch(\"TagModule/findAll\")\n            .then(() => {\n              this.closeDialog();\n            })\n            .catch((error) => {\n              this.$toast.error(`Kaydedilirken bir hata oluştu ${error}`, {\n                position: \"top-right\",\n              });\n            });\n        });\n      } else {\n        this.$store.dispatch(\"TagModule/save\", this.formData).then(() => {\n          this.$toast.success(\"Başarıyla Eklendi!\", {\n            position: \"top-right\",\n          });\n          this.$refs.form.reset();\n          this.$store\n            .dispatch(\"TagModule/findAll\")\n            .then(() => {\n              this.closeDialog();\n            })\n            .catch((error) => {\n              this.$toast.error(`Kaydedilirken bir hata oluştu ${error}`, {\n                position: \"top-right\",\n              });\n            });\n        });\n      }\n    },\n  },\n  data() {\n    return {\n      dialogModel: this.dialog,\n\n      formData: {\n        name: null,\n      },\n    };\n  },\n  watch: {\n    dialog() {\n      this.dialogModel = this.dialog;\n    },\n  },\n  created(){\n    if(this.edit){\n      this.$store.dispatch(\"TagModule/findById\",{id:this.tagId}).then((e)=>{\n        this.formData = e;\n      })\n    }\n  }\n};\n</script>\n\n<style>\n</style>\n"],"sourceRoot":"src/components/AddTagDialog"}]}