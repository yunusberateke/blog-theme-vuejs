A,KAAA,CAAA,uBAAA,CAAA,kBAAA;AACA;AAHA,GAfA;AAoBA,EAAA,OAAA,EAAA;AACA,IAAA,gBAAA,EAAA,0BAAA,IAAA,EAAA,MAAA,EAAA,cAAA,EAAA,aAAA,EAAA;AACA,UAAA,QAAA,GAAA,IAAA,QAAA,EAAA;AACA,MAAA,QAAA,CAAA,MAAA,CAAA,MAAA,EAAA,IAAA;AAEA,MAAA,YAAA,CACA,IADA,CACA,OADA,EACA,QADA,EAEA,IAFA,CAEA,UAAA,MAAA,EAAA;AACA,YAAA,GAAA,GAAA,mCAAA,MAAA,CAAA,IAAA,CAAA,IAAA,CADA,CACA;;AACA,QAAA,MAAA,CAAA,WAAA,CAAA,cAAA,EAAA,OAAA,EAAA,GAAA;AACA,QAAA,aAAA;AACA,OANA,EAOA,KAPA,CAOA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OATA;AAUA;AACA,KAhBA;AAiBA,IAAA,YAjBA,0BAiBA;AAAA;;AACA,WAAA,QAAA,CAAA,WAAA,GAAA,KAAA,OAAA;AACA,UAAA,KAAA,GAAA,IAAA,QAAA,EAAA;AACA,MAAA,KAAA,CAAA,MAAA,CAAA,MAAA,EAAA,KAAA,KAAA;AAEA,MAAA,YAAA,CACA,IADA,CACA,OADA,EACA,KADA,EAEA,IAFA,CAEA,UAAA,MAAA,EAAA;AACA,QAAA,KAAA,CAAA,QAAA,CAAA,KAAA,GACA,mCAAA,MAAA,CAAA,IAAA,CAAA,IADA,CADA,CAEA;;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA;;AACA,QAAA,KAAA,CAAA,MAAA,CACA,QADA,CACA,sBADA,EACA,KAAA,CAAA,QADA,EAEA,IAFA,CAEA,YAAA;AACA,UAAA,KAAA,CAAA,MAAA,CAAA,OAAA,CAAA,sBAAA,EAAA;AACA,YAAA,GAAA,EAAA,IADA;AAEA,YAAA,KAAA,EAAA;AAFA,WAAA;AAIA,SAPA,EAQA,KARA,CAQA,UAAA,KAAA,EAAA;AACA,UAAA,KAAA,CAAA,MAAA,CAAA,IAAA,8CAAA,KAAA,GAAA;AACA,YAAA,GAAA,EAAA,IADA;AAEA,YAAA,KAAA,EAAA;AAFA,WAAA;AAIA,SAbA;AAcA,OApBA,EAqBA,KArBA,CAqBA,UAAA,KAAA,EAAA;AACA,QAAA,KAAA,CAAA,MAAA,CAAA,IAAA,8CAAA,KAAA,GAAA;AACA,UAAA,GAAA,EAAA,IADA;AAEA,UAAA,KAAA,EAAA;AAFA,SAAA;AAIA,OA1BA;AA2BA;AAjDA,GApBA;AAuEA,EAAA,OAvEA,qBAuEA;AAAA;;AACA,SAAA,SAAA,GAAA,IAAA;AACA,SAAA,MAAA,CAAA,QAAA,CAAA,iCAAA,EAAA,IAAA,CAAA,YAAA;AACA,MAAA,MAAA,CAAA,SAAA,GAAA,KAAA;AACA,KAFA;AAGA;AA5EA,CAAA","sourcesContent":["<template>\n  <div>\n    <loader v-if=\"isLoading\"></loader>\n    <v-card class=\"col-12 col-md-11 col-lg-10 mx-auto\" v-if=\"!isLoading\">\n      <v-card-text style=\"min-height: 85vh\">\n        <v-form>\n          <v-file-input v-model=\"image\"></v-file-input>\n          <v-text-field label=\"İsmi\" v-model=\"formData.title\"></v-text-field>\n          <v-row class=\"ma-0 pa-0\">\n            <v-autocomplete\n              item-text=\"name\"\n              item-value=\"id\"\n              :items=\"categories\"\n              v-model=\"formData.category_id\"\n              label=\"Kategori\"\n            ></v-autocomplete>\n            <add-portfolio-category-dialog\n              class=\"my-auto ml-2\"\n              :dialog.sync=\"dialog\"\n              :categories=\"categories\"\n            ></add-portfolio-category-dialog>\n          </v-row>\n          <v-text-field label=\"Github Url\" v-model=\"formData.github_url\"></v-text-field>\n        </v-form>\n        <vue-editor\n          placeholder=\"İçerik\"\n          id=\"editor\"\n          use-custom-image-handler\n          @image-added=\"handleImageAdded\"\n          v-model=\"content\"\n          lang=\"tr\"\n          style=\"mim-height: 63vh\"\n        >\n        </vue-editor>\n        <v-row class=\"my-5\">\n          <v-btn\n            @click=\"saveFormData\"\n            outlined\n            class=\"my-2 mx-auto\"\n            color=\"primary\"\n            ><v-icon left>mdi-content-save</v-icon> Kaydet\n          </v-btn>\n        </v-row>\n      </v-card-text>\n    </v-card>\n  </div>\n</template>\n\n<script>\nimport { VueEditor } from \"vue2-editor\";\nimport serverClient from \"@/store/http_client\";\nimport Loader from \"@/components/Loader/loader.vue\";\nimport AddPortfolioCategoryDialog from \"@/components/AddPortfolioCategoryDialog/add-portfolio-category-dialog.vue\";\nexport default {\n  components: { VueEditor, Loader, AddPortfolioCategoryDialog },\n  data() {\n    return {\n      content: \"\",\n      image: null,\n      formData: {\n        title: null,\n        category_id: null,\n        github_url:null,\n      },\n      isLoading: false,\n      dialog: false,\n    };\n  },\n  computed: {\n    categories() {\n      return this.$store.state.PortfolioCategoryModule.portfolio_category;\n    },\n  },\n  methods: {\n    handleImageAdded: function (file, Editor, cursorLocation, resetUploader) {\n      var formData = new FormData();\n      formData.append(\"file\", file);\n\n      serverClient\n        .post(\"/file\", formData)\n        .then((result) => {\n          let url = \"http://localhost:8000/storage/\" + result.data.file; // Get url from response\n          Editor.insertEmbed(cursorLocation, \"image\", url);\n          resetUploader();\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n      0;\n    },\n    saveFormData() {\n      this.formData.description = this.content;\n      var image = new FormData();\n      image.append(\"file\", this.image);\n\n      serverClient\n        .post(\"/file\", image)\n        .then((result) => {\n          this.formData.image =\n            \"http://localhost:8000/storage/\" + result.data.file; // Get url from response\n          console.log(this.formData);\n          this.$store\n            .dispatch(\"PortfolioModule/save\", this.formData)\n            .then(() => {\n              this.$toast.success(\"Başarıyla kaydedildi\", {\n                top: true,\n                right: true,\n              });\n            })\n            .catch((error) => {\n              this.$toast.fail(`Kaydedilirken bir hata oluştu ${error}`, {\n                top: true,\n                right: true,\n              });\n            });\n        })\n        .catch((error) => {\n          this.$toast.fail(`Kaydedilirken bir hata oluştu ${error}`, {\n            top: true,\n            right: true,\n          });\n        });\n    },\n  },\n  mounted() {\n    this.isLoading = true;\n    this.$store.dispatch(\"PortfolioCategoryModule/findAll\").then(() => {\n      this.isLoading = false;\n    });\n  },\n};\n</script>\n\n<style>\n</style>"],"sourceRoot":"src/views/AdminTemplate"}]}